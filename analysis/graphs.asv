clear all;
close all;


data=readmatrix('legacy_mode_data.csv');

time = data(:, 1) / 1000;
averageOfSpeeds = data(:, 2);
singleSpeed = data(:, 3);
subplot(2,1,1);
plot(time, averageOfSpeeds)
title("Média das velocidades dos veículos");
ylabel("velocidade [km/h]");
xlabel("tempo [s]")
axis([0 120 0 60]);


subplot(2,1,2);
plot(time, singleSpeed)
title("Velocidade de um dos veículos");
axis([0 120 0 60]);